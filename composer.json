{
    "name": "nolin/laratools",
    "description": "Something I often use while coding a laravel project. So I made a package to build a new project.",
    "authors": [
        {
            "name": "Nolin",
            "email": "aaa913121@gmail.com",
            "homepage": "https://github.com/aaa913121"
        }
    ],
    "homepage": "https://github.com/aaa913121/laratools",
    "keywords": ["Laravel", "support"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "predis/predis": "^1.1",
        "webpatser/laravel-uuid": "^3.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nolin\\Laratools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nolin\\Laratools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nolin\\Laratools\\LaratoolsServiceProvider"
            ],
            "aliases": {
                "laratools": "Nolin\\Laratools\\Facades\\Support"
            }
        }
    }
}
