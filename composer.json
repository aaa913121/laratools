{
    "name": "nolin/laratools",
    "description": "Something I often use while coding a laravel project. So I made a package to build a new project.",
    "license": "license",
    "authors": [
        {
            "name": "Nolin",
            "email": "aaa913121@gmail.com",
            "homepage": "https://github.com/aaa913121"
        }
    ],
    "homepage": "https://github.com/aaa913121/laratools",
    "keywords": ["Laravel", "support"],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "predis/predis": "^1.1",
        "spatie/laravel-image-optimizer": "^1.6",
        "webpatser/laravel-uuid": "^3.0",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "maatwebsite/excel": "^3.1",
        "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "nolin\\laratools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nolin\\laratools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "nolin\\laratools\\SupportServiceProvider"
            ],
            "aliases": {
                "laratools": "nolin\\laratools\\Facades\\Support"
            }
        }
    },
    "minimum-stability": "dev"
}
